{
  "name": "vite-plugin-unocss-to-uni",
  "version": "0.0.13",
  "description": "Adapt unocss to uni-app",
  "author": "zguolee",
  "license": "MIT",
  "homepage": "https://github.com/zguolee/vite-plugin-unocss-to-uni/tree/main/#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/zguolee/vite-plugin-unocss-to-uni",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/zguolee/vite-plugin-unocss-to-uni/issues"
  },
  "keywords": [
    "vite",
    "vue",
    "vite-plugin",
    "unocss",
    "uni-app"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "play": "vite playground",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "test:update": "vitest -u",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.2.36",
    "magic-string": "^0.26.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.0",
    "@types/node": "^17.0.36",
    "bumpp": "^7.1.1",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.16.0",
    "esno": "^0.16.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.2",
    "unbuild": "^0.7.4",
    "vite": "^2.9.9",
    "vitest": "^0.13.0"
  }
}
